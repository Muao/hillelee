<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kuznetsov.entities.Questions" table="quizzes">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="qiuzId" type="java.lang.Integer" column="quizId"/>
        <property name="question" type="java.lang.String" column="subject"/>
        <property name="answer" type="java.lang.Integer" column="answer"/>

    </class>
</hibernate-mapping>