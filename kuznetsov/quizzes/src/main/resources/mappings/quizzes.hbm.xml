<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.kuznetsov.entities.Quizzes" table="quizzes">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="users" type="java.lang.Integer">
            <column name="login"/>
        </property>
        <property name="subjects" type="java.lang.Integer">
            <column name="subject"/>
        </property>
        <property name="themes" type="java.lang.Integer">
            <column name="theme"/>
        </property>
        <property name="questions" type="java.lang.Integer">
            <column name="questions"/>
        </property>
    </class>
</hibernate-mapping>