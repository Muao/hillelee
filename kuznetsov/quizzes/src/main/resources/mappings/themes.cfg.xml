<hibernate-mapping>
    <class name="com.kuznetsov.entities.Themes" table="quizzes">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="theme" type="java.lang.String">
            <column name="themes"/>
        </property>

    </class>
</hibernate-mapping>