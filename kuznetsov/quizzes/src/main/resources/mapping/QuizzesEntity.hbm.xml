<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kuznetsov.entities.Quizzes" table="quizzes" schema="quiz">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="subject">
            <column name="subject" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="theme">
            <column name="theme" sql-type="int(11)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>